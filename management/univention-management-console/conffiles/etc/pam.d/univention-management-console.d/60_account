@!@
unix_maximum_uid = int(configRegistry.get('pam/unix/maximum_uid', 2000))
print(f'''
account  [success=1 default=ignore]   pam_succeed_if.so uid > {unix_maximum_uid}''')
print('account  sufficient        pam_unix.so')
methods = configRegistry.get('auth/methods', '').split(' ')
if 'krb5' in methods:
    print('account  sufficient        pam_krb5.so force_pwchange')
print('account  required          pam_sss.so')
@!@
